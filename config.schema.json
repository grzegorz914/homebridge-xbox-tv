												"title": "Nexus",
												"enum": [
													"nexus"
												]
											},
											{
												"title": "View",
												"enum": [
													"view"
												]
											},
											{
												"title": "Menu",
												"enum": [
													"menu"
												]
											},
											{
												"title": "A",
												"enum": [
													"a"
												]
											},
											{
												"title": "B",
												"enum": [
													"b"
												]
											},
											{
