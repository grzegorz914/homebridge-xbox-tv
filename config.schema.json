													"rewind"
												]
											},
											{
												"title": "Channel Up",
												"enum": [
													"channelUp"
												]
											},
											{
												"title": "Channel Down",
												"enum": [
													"channelDown"
												]
											},
											{
												"title": "Back",
												"enum": [
													"back"
												]
											},
											{
												"title": "View",
												"enum": [
													"view"
												]
											},
											{
												"title": "Menu",
												"enum": [
													"menu"
												]
											},
											{
												"title": "Seek",
												"enum": [
													"seek"
												]
											}
										],
										"description": "Here select the media command.",
