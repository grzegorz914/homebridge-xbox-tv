									"type": "boolean",
									"default": false,
									"description": "This expose Input as a Motion Sensor."
								},
								"inputs": {
									"type": "array",
									"items": {
										"title": "Input",
										"type": "object",
										"description": "Here create Input/App Sensors based on reference, sensor will active if switch to it.",
										"properties": {
											"displayType": {
												"title": "Display Type",
												"type": "integer",
												"minimum": 0,
												"maximum": 3,
												"default": 0,
												"description": "Here select sensor type to be exposed in HomeKit app.",
												"oneOf": [
													{
														"title": "None/Disabled",
														"enum": [
															0
														]
													},
													{
														"title": "Motion Sensor",
														"enum": [
															1
														]
													},
													{
														"title": "Occupancy Sensor",
														"enum": [
															2
														]
													},
													{
														"title": "Contact Sensor",
														"enum": [
															3
														]
													}
												]
											},
											"name": {
												"title": "Name",
												"type": "string",
												"placeholder": "Name",
												"description": "Here set Your own name.",
												"condition": {
													"functionBody": "return model.devices[arrayIndices[0]].sensors.inputs[arrayIndices[1]].displayType > 0;"
												}
											},
											"reference": {
												"title": "Reference",
												"type": "string",
												"placeholder": "Input/App reference",
												"description": "Here set the Input/App reference.",
												"condition": {
													"functionBody": "return model.devices[arrayIndices[0]].sensors.inputs[arrayIndices[1]].displayType > 0;"
												}
											},
											"namePrefix": {
												"title": "Prefix",
												"type": "boolean",
												"default": false,
												"description": "Here enable/disable the accessory name as a prefix for sensor name.",
												"condition": {
													"functionBody": "return model.devices[arrayIndices[0]].sensors.inputs[arrayIndices[1]].displayType > 0;"
												}
											}
										},
										"required": [
											"displayType"
										]
									}
								}
							}
						},
