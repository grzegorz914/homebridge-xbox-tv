								"properties": {
									"name": {
										"title": "Name",
										"type": "string",
										"placeholder": "Name",
										"description": "Here set Your own name.",
										"required": false
									},
									"reference": {
										"title": "Reference",
										"type": "string",
										"placeholder": "Input/App reference",
										"description": "Here set the Input/App reference.",
