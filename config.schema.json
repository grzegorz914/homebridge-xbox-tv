												"enum": [
													0
												]
											},
											{
												"title": "Outlet",
												"enum": [
													1
												]
											},
											{
												"title": "Switch",
												"enum": [
													2
												]
											}
										],
										"description": "Here select display type in HomeKit app.",
										"required": true
									},
									"namePrefix": {
										"title": "Prefix",
										"type": "boolean",
										"description": "Here enable/disable the accessory name as a prefix for button name.",
										"required": false
									}
								}
							}
						},
						"sensorPower": {
